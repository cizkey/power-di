{
  "name": "power-di",
  "version": "1.0.7",
  "description": "A lightweight Dependency Injection library. Using es6 and other features, remove unnecessary concepts, easy and convenient to use.",
  "main": "index.js",
  "files": [
    "utils.js",
    "utils.d.ts",
    "helper.js",
    "helper.d.ts",
    "react.js",
    "react.d.ts",
    "dist"
  ],
  "scripts": {
    "clean:dist": "rimraf -- dist",
    "build:prod": "rimraf -- dist && tsc",
    "build:test": "rimraf -- __tests__ && tsc -p tsconfig.test.json",
    "test": "tsc && npm run build:test && ava -v -s",
    "test:prod": "npm run build:prod && npm run build:test && ava -v -s",
    "coverage": "nyc npm test",
    "coverage:report": "nyc report --reporter=lcov",
    "coverage:upload": "cat ./coverage/lcov.info | coveralls",
    "report": "npm run coverage && npm run coverage:report",
    "reinstall": "rimraf -- node_modules && npm install",
    "prepublish": "npm run build:prod"
  },
  "ava": {
    "files": [
      "__tests__/*.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhang740/power-di.git"
  },
  "author": "zhang740",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zhang740/power-di/issues"
  },
  "homepage": "https://github.com/zhang740/power-di#readme",
  "typings": "./dist/index.d.ts",
  "dependencies": {},
  "keywords": [
    "di",
    "ts",
    "ioc",
    "react"
  ],
  "devDependencies": {
    "react": "^15.4.2",
    "@types/react": "^15.0.16",
    "@types/react-test-renderer": "^15.4.2",
    "ava": "^0.18.2",
    "rimraf": "^2.6.1",
    "coveralls": "^2.12.0",
    "nyc": "^10.1.2",
    "react-test-renderer": "^15.4.2",
    "tslint": "4.5.1",
    "typescript": "2.2.1"
  }
}