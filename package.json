{
  "name": "power-di",
  "version": "1.3.5",
  "description": "A lightweight Dependency Injection library. Using es6 and other features, remove unnecessary concepts, easy and convenient to use.",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "files": [
    "utils.js",
    "utils.d.ts",
    "helper.js",
    "helper.d.ts",
    "react.js",
    "react.d.ts",
    "lib",
    "dist"
  ],
  "scripts": {
    "clean": "rimraf -- dist lib/**/*.js lib/**/*.js.map lib/**/*.d.ts",
    "babel": "babel lib --out-dir lib",
    "build:dist": "webpack --progress",
    "test": "rimraf -- test/**/*.js && tsc && ava -v -s",
    "test:prod": "tsc && npm run babel && npm run test",
    "coverage": "nyc npm test",
    "coverage:report": "nyc report --reporter=lcov",
    "coverage:upload": "cat ./coverage/lcov.info | coveralls",
    "report": "npm run coverage && npm run coverage:report",
    "reinstall": "rimraf -- node_modules && npm install",
    "prepublish": "npm run clean && npm run test:prod",
    "postpublish": "node scripts/published.js"
  },
  "ava": {
    "files": [
      "test/*.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhang740/power-di.git"
  },
  "author": "zhang740",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zhang740/power-di/issues"
  },
  "homepage": "https://github.com/zhang740/power-di#readme",
  "dependencies": {
    "reflect-metadata": "^0.1.12"
  },
  "keywords": [
    "di",
    "ts",
    "ioc",
    "react"
  ],
  "devDependencies": {
    "react": "^15.6.1",
    "@types/node": "^9.4.7",
    "@types/react": "^15.6.0",
    "@types/react-test-renderer": "^15.5.2",
    "ava": "^0.21.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "rimraf": "^2.6.1",
    "coveralls": "^2.13.1",
    "nyc": "^11.1.0",
    "react-test-renderer": "^15.6.1",
    "tslint": "5.5.0",
    "typescript": "2.4.2",
    "webpack": "^3.6.0"
  }
}
